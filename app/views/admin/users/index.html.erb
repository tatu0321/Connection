<h1>会員一覧</h1>
<table>
  <tr>
    <th>名前</th>
    <th>メール</th>
    <th>詳細</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= link_to '詳細', admin_user_path(user) %></td>
    </tr>
  <% end %>
</table>
<%= link_to "logout", destroy_admin_session_path %>